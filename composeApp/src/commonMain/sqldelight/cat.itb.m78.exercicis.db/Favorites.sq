CREATE TABLE favorites (
    Pokemon_id INTEGER NOT NULL,
    Name TEXT NOT NULL,
    Habitat TEXT,
    Img TEXT
);

CREATE INDEX favorites_name ON favorites(Name);
selectAll:
SELECT * FROM favorites;
insert:
INSERT INTO favorites(Pokemon_id, Name, Habitat, Img) VALUES (?, ?, ?, ?);
find:
SELECT * FROM favorites WHERE Pokemon_id = ?;
delete:
DELETE FROM favorites WHERE Pokemon_id = ?;
selectIds:
SELECT Pokemon_id FROM favorites;